{
    "name": "whiskertoolbox",
    "version-string": "0.0.1",
    "dependencies": [
        {
            "name": "armadillo"
        },
        {
            "name": "glib"
        },
        {
            "name": "glm"
        },
        {
            "name": "lapack"
        },
        {
            "name": "libffi",
            "platform": "osx"
        },
        {   "name": "nlohmann-json"
        },
        {
            "name": "openblas"
        },
        {
            "name": "eigen3"
        }

    ],
    "features": {
        "capnproto": {
            "description": "CapnProto serialization support for binary data formats",
            "dependencies": [
                "capnproto"
            ]
        },
        "hdf5": {
            "description": "HDF5 support for hierarchical data format loading/saving",
            "dependencies": [
                {
                    "name": "hdf5",
                    "features": ["cpp"]
                },
                "highfive"
            ]
        },
        "ffmpeg": {
            "description": "FFmpeg support for video loading and processing",
            "dependencies": [
                {
                    "name": "ffmpeg",
                    "default-features": true,
                    "features": [
                        {
                            "name": "nvcodec",
                            "platform": "!osx"
                        }
                    ]
                },
                {
                    "name": "boost-circular-buffer"
                }
            ]
        },
        "ui": {
            "description": "User interface components",
            "dependencies": [
                {
                    "name": "mlpack"
                },
                {
                    "name": "opencv4"
                }
            ]
        }
    },
    "builtin-baseline": "3508985146f1b1d248c67ead13f8f54be5b4f5da",
    "$comment": "hash corresponds to 2024.08.23",
    "overrides": [
        {
            "name": "armadillo",
            "version": "12.8.4"
        },
        {
            "name": "boost-circular-buffer",
            "version": "1.85.0"
        },
        {
            "name": "capnproto",
            "version": "1.0.1"
        },
        {
            "name": "ffmpeg",
            "version": "6.1.1#9"
        },
        {
            "name": "glib",
            "version": "2.80.0",
            "$comment": "glib below this version requires distutils in python, which is removed in >=3.12.",
            "$comment2": "This version was added 2024.10.09"
        },
        {
            "name": "glm",
            "version": "1.0.1#3"
        },
        {
            "name": "hdf5",
            "version": "1.14.4.3"
        },
        {
            "name": "highfive",
            "version": "2.10.0"
        },
        {
            "name": "lapack",
            "version": "2023-06-10"
        },
        {
            "name": "libffi",
            "version": "3.4.7"
        },
        {
            "name": "mlpack",
            "version": "4.4.0"
        },
        {   "name": "nlohmann-json",
            "version": "3.11.3"
        },
        {
            "name": "openblas",
            "version": "0.3.27"
        },
        {
            "name": "opencv4",
            "version": "4.8.0"
        }
        ]

}
