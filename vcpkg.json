{
    "name": "whiskertoolbox",
    "version-string": "0.0.1",
    "dependencies": [
        {
            "name": "armadillo"
        },
        {
            "name": "eigen3"
        },
        {
            "name": "glib"
        },
        {
            "name": "glm"
        },
        {
            "name": "lapack"
        },
        {
            "name": "libffi",
            "platform": "osx"
        },
        {
            "name": "nlohmann-json"
        },
        {
            "name": "openblas"
        },
        {
            "name": "reflectcpp"
        },
        {
            "name": "spdlog"
        }
    ],
    "features": {
        "capnproto": {
            "description": "CapnProto serialization support for binary data formats",
            "dependencies": [
                "capnproto"
            ]
        },
        "hdf5": {
            "description": "HDF5 support for hierarchical data format loading/saving",
            "dependencies": [
                {
                    "name": "hdf5",
                    "features": [
                        "cpp"
                    ]
                },
                "highfive"
            ]
        },
        "ffmpeg": {
            "description": "FFmpeg support for video loading and processing",
            "dependencies": [
                {
                    "name": "ffmpeg",
                    "default-features": true,
                    "features": [
                        {
                            "name": "nvcodec",
                            "platform": "!osx"
                        }
                    ]
                },
                {
                    "name": "boost-circular-buffer"
                }
            ]
        },
        "ui": {
            "description": "User interface components",
            "dependencies": [
                {
                    "name": "mlpack"
                },
                {
                    "name": "opencv4"
                },
                {
                    "name": "protobuf"
                },
                {
                    "name": "abseil",
                    "features": [
                        {
                            "name": "cxx17"
                        }
                    ]
                }
            ]
        },
        "ortools": {
            "description": "Google OR-Tools for optimization problems",
            "dependencies": [
                {
                    "name": "boost-multiprecision"
                },
                {
                    "name": "boost-serialization"
                },
                {
                    "name": "boost-random"
                },
                {
                    "name": "protobuf"
                },
                {
                    "name": "abseil",
                    "features": [
                        {
                            "name": "cxx17"
                        }
                    ]
                }
            ]
        }
    },
    "builtin-baseline": "b322364f06308bdd24823f9d8f03fe0cc86fd46f",
    "$comment": "hash corresponds to 2024.12.16",
    "overrides": [
        {
            "name": "abseil",
            "version": "20240722.0"
        },
        {
            "name": "armadillo",
            "version": "12.8.4"
        },
        {
            "name": "boost-circular-buffer",
            "version": "1.85.0"
        },
        {
            "name": "capnproto",
            "version": "1.0.1"
        },
        {
            "name": "eigen3",
            "version": "3.4.0#4"
        },
        {
            "name": "ffmpeg",
            "version": "7.1#1"
        },
        {
            "name": "glib",
            "version": "2.80.0",
            "$comment": "glib below this version requires distutils in python, which is removed in >=3.12.",
            "$comment2": "This version was added 2024.10.09"
        },
        {
            "name": "glm",
            "version": "1.0.1#3"
        },
        {
            "name": "hdf5",
            "version": "1.14.4.3#3"
        },
        {
